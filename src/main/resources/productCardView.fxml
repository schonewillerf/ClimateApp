<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root prefHeight="300.0" prefWidth="200.0" style="-fx-background-color: white;" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <Label text="Product:">
      <VBox.margin>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </VBox.margin>
   </Label>
   <Label fx:id="productNameLabel" text="ProductNamePlaceholder">
      <VBox.margin>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </VBox.margin>
   </Label>
   <Label layoutX="10.0" layoutY="10.0" text="CO2 waarde:">
      <VBox.margin>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </VBox.margin>
   </Label>
   <Label fx:id="carbonAmountLabel" layoutX="10.0" layoutY="10.0" text="CarbonAmountPlaceholder">
      <VBox.margin>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </VBox.margin>
   </Label>
   <Button fx:id="addButton" mnemonicParsing="false" text="Toevoegen aan project">
      <VBox.margin>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </VBox.margin>
   </Button>
</fx:root>
