<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<fx:root prefHeight="232.0" prefWidth="200.0" style="-fx-background-color: white; -fx-border-color: black;" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <Label fx:id="productNameLabel" text="ProductNamePlaceholder">
      <VBox.margin>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </VBox.margin>
   </Label>
   <ImageView fx:id="productImage" fitHeight="154.0" fitWidth="137.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@test123.jpg" />
      </image>
      <VBox.margin>
         <Insets left="27.0" />
      </VBox.margin>
   </ImageView>
   <Button fx:id="addButton" mnemonicParsing="false" text="Toevoegen aan project">
      <VBox.margin>
         <Insets bottom="10.0" left="27.0" right="10.0" top="10.0" />
      </VBox.margin>
   </Button>
</fx:root>
