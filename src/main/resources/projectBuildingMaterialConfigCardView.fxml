<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<fx:root alignment="TOP_CENTER" prefHeight="200.0" prefWidth="200.0" spacing="10.0" style="-fx-background-color: #f4f4f4;" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <ComboBox fx:id="circulationDropdown" prefWidth="150.0" />
      <Label fx:id="embodiedCarbonLabel" text="Embodied Carbon: " />
      <Label fx:id="embodiedEnergyLabel" text="Embodied Energy: " />
   </children>
</fx:root>
